{{#paper-toolbar}}
  {{#paper-toolbar-tools class="title-bar"}}
    {{#paper-sidenav-toggle as |menu|}}
      {{#paper-button onClick=(action "toggleMenu" target=menu) iconButton=true}}
        {{paper-icon "menu"}}
      {{/paper-button}}
    {{/paper-sidenav-toggle}}

    {{yield}}
  {{/paper-toolbar-tools}}
{{/paper-toolbar}}
